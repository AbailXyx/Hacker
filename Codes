local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local LocalPlayer = game:GetService("Players").LocalPlayer
local Mouse = LocalPlayer:GetMouse()
local HttpService = game:GetService("HttpService")

local NexusUI = {
	Elements = {},
	ThemeObjects = {},
	Connections = {},
	Flags = {},
	Themes = {
		Default = {
			Main = Color3.fromRGB(20, 20, 20),
			Second = Color3.fromRGB(30, 30, 35),
			Stroke = Color3.fromRGB(70, 0, 255),
			Divider = Color3.fromRGB(40, 40, 45),
			Text = Color3.fromRGB(240, 240, 240),
			TextDark = Color3.fromRGB(180, 180, 185)
		},
		Dark = {
			Main = Color3.fromRGB(15, 15, 15),
			Second = Color3.fromRGB(25, 25, 30),
			Stroke = Color3.fromRGB(0, 170, 255),
			Divider = Color3.fromRGB(35, 35, 40),
			Text = Color3.fromRGB(240, 240, 240),
			TextDark = Color3.fromRGB(170, 170, 175)
		}
	},
	SelectedTheme = "Default",
	Folder = nil,
	SaveCfg = false
}

--Feather Icons
local Icons = {}

local Success, Response = pcall(function()
	Icons = HttpService:JSONDecode(game:HttpGetAsync("https://raw.githubusercontent.com/evoincorp/lucideblox/master/src/modules/util/icons.json")).icons
end)

if not Success then
	warn("\nNexus Library - Failed to load Feather Icons. Error code: " .. Response .. "\n")
end	

local function GetIcon(IconName)
	if Icons[IconName] ~= nil then
		return Icons[IconName]
	else
		return nil
	end
end   

local Nexus = Instance.new("ScreenGui")
Nexus.Name = "Nexus"
if syn then
	syn.protect_gui(Nexus)
	Nexus.Parent = game.CoreGui
else
	Nexus.Parent = gethui() or game.CoreGui
end

if gethui then
	for _, Interface in ipairs(gethui():GetChildren()) do
		if Interface.Name == Nexus.Name and Interface ~= Nexus then
			Interface:Destroy()
		end
	end
else
	for _, Interface in ipairs(game.CoreGui:GetChildren()) do
		if Interface.Name == Nexus.Name and Interface ~= Nexus then
			Interface:Destroy()
		end
	end
end

function NexusUI:IsRunning()
	if gethui then
		return Nexus.Parent == gethui()
	else
		return Nexus.Parent == game:GetService("CoreGui")
	end
end

local function AddConnection(Signal, Function)
	if (not NexusUI:IsRunning()) then
		return
	end
	local SignalConnect = Signal:Connect(Function)
	table.insert(NexusUI.Connections, SignalConnect)
	return SignalConnect
end

task.spawn(function()
	while (NexusUI:IsRunning()) do
		wait()
	end

	for _, Connection in next, NexusUI.Connections do
		Connection:Disconnect()
	end
end)

local function AddDraggingFunctionality(DragPoint, Main)
	pcall(function()
		local Dragging, DragInput, MousePos, FramePos = false
		DragPoint.InputBegan:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
				Dragging = true
				MousePos = Input.Position
				FramePos = Main.Position

				Input.Changed:Connect(function()
					if Input.UserInputState == Enum.UserInputState.End then
						Dragging = false
					end
				end)
			end
		end)
		DragPoint.InputChanged:Connect(function(Input)
			if Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch then
				DragInput = Input
			end
		end)
		UserInputService.InputChanged:Connect(function(Input)
			if Input == DragInput and Dragging then
				local Delta = Input.Position - MousePos
				TweenService:Create(Main, TweenInfo.new(0.45, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position  = UDim2.new(FramePos.X.Scale,FramePos.X.Offset + Delta.X, FramePos.Y.Scale, FramePos.Y.Offset + Delta.Y)}):Play()
			end
		end)
	end)
end   

local function Create(Name, Properties, Children)
	local Object = Instance.new(Name)
	for i, v in next, Properties or {} do
		Object[i] = v
	end
	for i, v in next, Children or {} do
		v.Parent = Object
	end
	return Object
end

local function CreateElement(ElementName, ElementFunction)
	NexusUI.Elements[ElementName] = function(...)
		return ElementFunction(...)
	end
end

local function MakeElement(ElementName, ...)
	local NewElement = NexusUI.Elements[ElementName](...)
	return NewElement
end

local function SetProps(Element, Props)
	table.foreach(Props, function(Property, Value)
		Element[Property] = Value
	end)
	return Element
end

local function SetChildren(Element, Children)
	table.foreach(Children, function(_, Child)
		Child.Parent = Element
	end)
	return Element
end

local function Round(Number, Factor)
	local Result = math.floor(Number/Factor + (math.sign(Number) * 0.5)) * Factor
	if Result < 0 then Result = Result + Factor end
	return Result
end

local function ReturnProperty(Object)
	if Object:IsA("Frame") or Object:IsA("TextButton") then
		return "BackgroundColor3"
	end 
	if Object:IsA("ScrollingFrame") then
		return "ScrollBarImageColor3"
	end 
	if Object:IsA("UIStroke") then
		return "Color"
	end 
	if Object:IsA("TextLabel") or Object:IsA("TextBox") then
		return "TextColor3"
	end   
	if Object:IsA("ImageLabel") or Object:IsA("ImageButton") then
		return "ImageColor3"
	end   
end

local function AddThemeObject(Object, Type)
	if not NexusUI.ThemeObjects[Type] then
		NexusUI.ThemeObjects[Type] = {}
	end    
	table.insert(NexusUI.ThemeObjects[Type], Object)
	Object[ReturnProperty(Object)] = NexusUI.Themes[NexusUI.SelectedTheme][Type]
	return Object
end    

local function SetTheme()
	for Name, Type in pairs(NexusUI.ThemeObjects) do
		for _, Object in pairs(Type) do
			Object[ReturnProperty(Object)] = NexusUI.Themes[NexusUI.SelectedTheme][Name]
		end    
	end    
end

local function PackColor(Color)
	return {R = Color.R * 255, G = Color.G * 255, B = Color.B * 255}
end    

local function UnpackColor(Color)
	return Color3.fromRGB(Color.R, Color.G, Color.B)
end

local function LoadCfg(Config)
	local Data = HttpService:JSONDecode(Config)
	table.foreach(Data, function(a,b)
		if NexusUI.Flags[a] then
			spawn(function() 
				if NexusUI.Flags[a].Type == "Colorpicker" then
					NexusUI.Flags[a]:Set(UnpackColor(b))
				else
					NexusUI.Flags[a]:Set(b)
				end    
			end)
		else
			warn("Nexus Library Config Loader - Could not find ", a ,b)
		end
	end)
end

local function SaveCfg(Name)
	local Data = {}
	for i,v in pairs(NexusUI.Flags) do
		if v.Save then
			if v.Type == "Colorpicker" then
				Data[i] = PackColor(v.Value)
			else
				Data[i] = v.Value
			end
		end	
	end
	writefile(NexusUI.Folder .. "/" .. Name .. ".txt", tostring(HttpService:JSONEncode(Data)))
end

local WhitelistedMouse = {Enum.UserInputType.MouseButton1, Enum.UserInputType.MouseButton2,Enum.UserInputType.MouseButton3,Enum.UserInputType.Touch}
local BlacklistedKeys = {Enum.KeyCode.Unknown,Enum.KeyCode.W,Enum.KeyCode.A,Enum.KeyCode.S,Enum.KeyCode.D,Enum.KeyCode.Up,Enum.KeyCode.Left,Enum.KeyCode.Down,Enum.KeyCode.Right,Enum.KeyCode.Slash,Enum.KeyCode.Tab,Enum.KeyCode.Backspace,Enum.KeyCode.Escape}

local function CheckKey(Table, Key)
	for _, v in next, Table do
		if v == Key then
			return true
		end
	end
end

CreateElement("Corner", function(Scale, Offset)
	local Corner = Create("UICorner", {
		CornerRadius = UDim.new(Scale or 0, Offset or 10)
	})
	return Corner
end)

CreateElement("Stroke", function(Color, Thickness)
	local Stroke = Create("UIStroke", {
		Color = Color or Color3.fromRGB(255, 255, 255),
		Thickness = Thickness or 1
	})
	return Stroke
end)

CreateElement("List", function(Scale, Offset)
	local List = Create("UIListLayout", {
		SortOrder = Enum.SortOrder.LayoutOrder,
		Padding = UDim.new(Scale or 0, Offset or 0)
	})
	return List
end)

CreateElement("Padding", function(Bottom, Left, Right, Top)
	local Padding = Create("UIPadding", {
		PaddingBottom = UDim.new(0, Bottom or 4),
		PaddingLeft = UDim.new(0, Left or 4),
		PaddingRight = UDim.new(0, Right or 4),
		PaddingTop = UDim.new(0, Top or 4)
	})
	return Padding
end)

CreateElement("TFrame", function()
	local TFrame = Create("Frame", {
		BackgroundTransparency = 1
	})
	return TFrame
end)

CreateElement("Frame", function(Color)
	local Frame = Create("Frame", {
		BackgroundColor3 = Color or Color3.fromRGB(255, 255, 255),
		BorderSizePixel = 0
	})
	return Frame
end)

CreateElement("RoundFrame", function(Color, Scale, Offset)
	local Frame = Create("Frame", {
		BackgroundColor3 = Color or Color3.fromRGB(255, 255, 255),
		BorderSizePixel = 0
	}, {
		Create("UICorner", {
			CornerRadius = UDim.new(Scale, Offset)
		})
	})
	return Frame
end)

CreateElement("Button", function()
	local Button = Create("TextButton", {
		Text = "",
		AutoButtonColor = false,
		BackgroundTransparency = 1,
		BorderSizePixel = 0
	})
	return Button
end)

CreateElement("ScrollFrame", function(Color, Width)
	local ScrollFrame = Create("ScrollingFrame", {
		BackgroundTransparency = 1,
		MidImage = "rbxassetid://7445543667",
		BottomImage = "rbxassetid://7445543667",
		TopImage = "rbxassetid://7445543667",
		ScrollBarImageColor3 = Color,
		BorderSizePixel = 0,
		ScrollBarThickness = Width,
		CanvasSize = UDim2.new(0, 0, 0, 0)
	})
	return ScrollFrame
end)

CreateElement("Image", function(ImageID)
	local ImageNew = Create("ImageLabel", {
		Image = ImageID,
		BackgroundTransparency = 1
	})

	if GetIcon(ImageID) ~= nil then
		ImageNew.Image = GetIcon(ImageID)
	end	

	return ImageNew
end)

CreateElement("ImageButton", function(ImageID)
	local Image = Create("ImageButton", {
		Image = ImageID,
		BackgroundTransparency = 1
	})
	return Image
end)

CreateElement("Label", function(Text, TextSize, Transparency)
	local Label = Create("TextLabel", {
		Text = Text or "",
		TextColor3 = Color3.fromRGB(240, 240, 240),
		TextTransparency = Transparency or 0,
		TextSize = TextSize or 15,
		Font = Enum.Font.Gotham,
		RichText = true,
		BackgroundTransparency = 1,
		TextXAlignment = Enum.TextXAlignment.Left
	})
	return Label
end)

local NotificationHolder = SetProps(SetChildren(MakeElement("TFrame"), {
	SetProps(MakeElement("List"), {
		HorizontalAlignment = Enum.HorizontalAlignment.Center,
		SortOrder = Enum.SortOrder.LayoutOrder,
		VerticalAlignment = Enum.VerticalAlignment.Bottom,
		Padding = UDim.new(0, 5)
	})
}), {
	Position = UDim2.new(1, -25, 1, -25),
	Size = UDim2.new(0, 300, 1, -25),
	AnchorPoint = Vector2.new(1, 1),
	Parent = Nexus
})

function NexusUI:MakeNotification(NotificationConfig)
	spawn(function()
		NotificationConfig.Name = NotificationConfig.Name or "Notification"
		NotificationConfig.Content = NotificationConfig.Content or "Test"
		NotificationConfig.Image = NotificationConfig.Image or "rbxassetid://4384403532"
		NotificationConfig.Time = NotificationConfig.Time or 15

		local NotificationParent = SetProps(MakeElement("TFrame"), {
			Size = UDim2.new(1, 0, 0, 0),
			AutomaticSize = Enum.AutomaticSize.Y,
			Parent = NotificationHolder
		})

		local NotificationFrame = SetChildren(SetProps(MakeElement("RoundFrame", Color3.fromRGB(25, 25, 25), 0, 12), {
			Parent = NotificationParent, 
			Size = UDim2.new(1, 0, 0, 0),
			Position = UDim2.new(1, -55, 0, 0),
			BackgroundTransparency = 0,
			AutomaticSize = Enum.AutomaticSize.Y
		}), {
			MakeElement("Stroke", Color3.fromRGB(70, 0, 255), 1.2),
			MakeElement("Padding", 12, 12, 12, 12),
			SetProps(MakeElement("Image", NotificationConfig.Image), {
				Size = UDim2.new(0, 20, 0, 20),
				ImageColor3 = Color3.fromRGB(240, 240, 240),
				Name = "Icon"
			}),
			SetProps(MakeElement("Label", NotificationConfig.Name, 15), {
				Size = UDim2.new(1, -30, 0, 20),
				Position = UDim2.new(0, 30, 0, 0),
				Font = Enum.Font.GothamBold,
				Name = "Title"
			}),
			SetProps(MakeElement("Label", NotificationConfig.Content, 14), {
				Size = UDim2.new(1, 0, 0, 0),
				Position = UDim2.new(0, 0, 0, 25),
				Font = Enum.Font.GothamSemibold,
				Name = "Content",
				AutomaticSize = Enum.AutomaticSize.Y,
				TextColor3 = Color3.fromRGB(200, 200, 200),
				TextWrapped = true
			})
		})

		TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {Position = UDim2.new(0, 0, 0, 0)}):Play()

		wait(NotificationConfig.Time)
		
		-- Fade out animasi
		TweenService:Create(NotificationFrame.Icon, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {ImageTransparency = 1}):Play()
		TweenService:Create(NotificationFrame.Title, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {TextTransparency = 1}):Play()
		TweenService:Create(NotificationFrame.Content, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {TextTransparency = 1}):Play()
		TweenService:Create(NotificationFrame.UIStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {Transparency = 1}):Play()
		TweenService:Create(NotificationFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad), {BackgroundTransparency = 1}):Play()
		
		wait(0.3) -- Tunggu animasi fade out selesai
		
		-- Slide out ke kanan
		TweenService:Create(NotificationFrame, TweenInfo.new(0.5, Enum.EasingStyle.Quad), {
			Position = UDim2.new(1, 20, 0, 0)
		}):Play()
		
		wait(0.5) -- Tunggu animasi slide out selesai
		
		-- Hapus notifikasi
		NotificationParent:Destroy()
	end)
end    

function NexusUI:Init()
	if NexusUI.SaveCfg then	
		pcall(function()
			if isfile(NexusUI.Folder .. "/" .. game.GameId .. ".txt") then
				LoadCfg(readfile(NexusUI.Folder .. "/" .. game.GameId .. ".txt"))
				NexusUI:MakeNotification({
					Name = "Configuration",
					Content = "Auto-loaded configuration for the game " .. game.GameId .. ".",
					Time = 5
				})
			end
		end)		
	end	
end	

function NexusUI:MakeWindow(WindowConfig)
	local FirstTab = true
	local Minimized = false
	local Loaded = false
	local UIHidden = false

	WindowConfig = WindowConfig or {}
	WindowConfig.Name = WindowConfig.Name or "Nexus UI"
	WindowConfig.ConfigFolder = WindowConfig.ConfigFolder or WindowConfig.Name
	WindowConfig.SaveConfig = WindowConfig.SaveConfig or false
	WindowConfig.HidePremium = WindowConfig.HidePremium or false
	if WindowConfig.IntroEnabled == nil then
		WindowConfig.IntroEnabled = true
	end
	WindowConfig.IntroText = WindowConfig.IntroText or "Nexus UI"
	WindowConfig.CloseCallback = WindowConfig.CloseCallback or function() end
	WindowConfig.ShowIcon = WindowConfig.ShowIcon or false
	WindowConfig.Icon = WindowConfig.Icon or "rbxassetid://8834748103"
	WindowConfig.IntroIcon = WindowConfig.IntroIcon or "rbxassetid://8834748103"
	NexusUI.Folder = WindowConfig.ConfigFolder
	NexusUI.SaveCfg = WindowConfig.SaveConfig

	if WindowConfig.SaveConfig then
		if not isfolder(WindowConfig.ConfigFolder) then
			makefolder(WindowConfig.ConfigFolder)
		end	
	end

	local CloseBtn = SetChildren(SetProps(MakeElement("Button"), {
		Size = UDim2.new(0.5, 0, 1, 0),
		Position = UDim2.new(0.5, 0, 0, 0),
		BackgroundTransparency = 1
	}), {
		SetProps(MakeElement("Image", "rbxassetid://7072725342"), {
			Position = UDim2.new(0, 9, 0, 6),
			Size = UDim2.new(0, 18, 0, 18),
			ImageColor3 = Color3.fromRGB(255, 255, 255)
		})
	})

	local MinimizeBtn = SetChildren(SetProps(MakeElement("Button"), {
		Size = UDim2.new(0.5, 0, 1, 0),
		BackgroundTransparency = 1
	}), {
		SetProps(MakeElement("Image", "rbxassetid://7072719338"), {
			Position = UDim2.new(0, 9, 0, 6),
			Size = UDim2.new(0, 18, 0, 18),
			Name = "Ico",
			ImageColor3 = Color3.fromRGB(255, 255, 255)
		})
	})

	local DragPoint = SetProps(MakeElement("TFrame"), {
		Size = UDim2.new(1, 0, 0, 50)
	})

	-- Store tabs for search functionality
	local TabButtons = {}

	-- ==============================================
	-- DESAIN UTAMA WINDOW - RED BLACK THEME
	-- ==============================================
	local WindowContainer = Instance.new("ImageLabel")
	WindowContainer.Name = "MainContainer"
	WindowContainer.Image = "http://www.roblox.com/asset/?id=78218407205334"
	WindowContainer.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	WindowContainer.BorderSizePixel = 0
	WindowContainer.Size = UDim2.new(0, 536, 0, 282)
	WindowContainer.Position = UDim2.new(0.5, -268, 0.5, -141)
	WindowContainer.ClipsDescendants = true
	
	-- Corner
	local UICorner = Instance.new("UICorner", WindowContainer)
	UICorner.CornerRadius = UDim.new(0, 8)
	
	-- Stroke merah
	local UIStroke = Instance.new("UIStroke", WindowContainer)
	UIStroke.Thickness = 2
	UIStroke.Color = Color3.fromRGB(255, 0, 0)
	
	-- Garis vertikal merah (pemisah tab)
	local VerticalLine = Instance.new("Frame", WindowContainer)
	VerticalLine.BorderSizePixel = 0
	VerticalLine.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	VerticalLine.Size = UDim2.new(0, 2, 0, 226)
	VerticalLine.Position = UDim2.new(0, 130, 0, 56)
	
	-- Garis horizontal merah (pemisah header)
	local HorizontalLine = Instance.new("Frame", WindowContainer)
	HorizontalLine.BorderSizePixel = 0
	HorizontalLine.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	HorizontalLine.Size = UDim2.new(0, 536, 0, 2)
	HorizontalLine.Position = UDim2.new(0, 0, 0, 52)
	
	-- Area konten utama (kanan)
	local ContentScroll = Instance.new("ScrollingFrame", WindowContainer)
	ContentScroll.BorderSizePixel = 0
	ContentScroll.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ContentScroll.Size = UDim2.new(0, 404, 0, 232)
	ContentScroll.ScrollBarImageColor3 = Color3.fromRGB(255, 0, 0)
	ContentScroll.Position = UDim2.new(0, 130, 0, 52)
	ContentScroll.ScrollBarThickness = 5
	ContentScroll.BackgroundTransparency = 1
	ContentScroll.CanvasSize = UDim2.new(0, 0, 0, 0)
	
	local UIPadding = Instance.new("UIPadding", ContentScroll)
	UIPadding.PaddingLeft = UDim.new(0, 12)
	UIPadding.PaddingRight = UDim.new(0, 12)
	UIPadding.PaddingTop = UDim.new(0, 12)
	UIPadding.PaddingBottom = UDim.new(0, 12)
	
	-- Area tab (kiri)
	local TabScroll = Instance.new("ScrollingFrame", WindowContainer)
	TabScroll.BorderSizePixel = 0
	TabScroll.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TabScroll.Size = UDim2.new(0, 130, 0, 232)
	TabScroll.ScrollBarImageColor3 = Color3.fromRGB(255, 0, 0)
	TabScroll.Position = UDim2.new(0, 0, 0, 52)
	TabScroll.ScrollBarThickness = 5
	TabScroll.BackgroundTransparency = 1
	TabScroll.CanvasSize = UDim2.new(0, 0, 0, 0)
	
	local TabList = Instance.new("UIListLayout", TabScroll)
	TabList.SortOrder = Enum.SortOrder.LayoutOrder
	TabList.Padding = UDim.new(0, 8)
	
	local TabPadding = Instance.new("UIPadding", TabScroll)
	TabPadding.PaddingLeft = UDim.new(0, 8)
	TabPadding.PaddingRight = UDim.new(0, 8)
	TabPadding.PaddingTop = UDim.new(0, 8)
	TabPadding.PaddingBottom = UDim.new(0, 8)

	-- Header dengan tombol close/minimize
	local HeaderFrame = Instance.new("Frame", WindowContainer)
	HeaderFrame.BorderSizePixel = 0
	HeaderFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	HeaderFrame.Size = UDim2.new(1, 0, 0, 52)
	HeaderFrame.Name = "Header"
	
	-- Title
	local TitleLabel = Instance.new("TextLabel", HeaderFrame)
	TitleLabel.BorderSizePixel = 0
	TitleLabel.TextSize = 20
	TitleLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TitleLabel.FontFace = Font.new("rbxasset://fonts/families/GothamSSm.json", Enum.FontWeight.Black, Enum.FontStyle.Normal)
	TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TitleLabel.BackgroundTransparency = 1
	TitleLabel.Size = UDim2.new(1, -100, 1, 0)
	TitleLabel.Position = UDim2.new(0, 25, 0, 0)
	TitleLabel.Text = WindowConfig.Name
	TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
	
	-- Close/Minimize buttons container
	local ControlButtons = Instance.new("Frame", HeaderFrame)
	ControlButtons.BorderSizePixel = 0
	ControlButtons.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	ControlButtons.Size = UDim2.new(0, 70, 0, 30)
	ControlButtons.Position = UDim2.new(1, -80, 0, 10)
	
	local ControlCorner = Instance.new("UICorner", ControlButtons)
	ControlCorner.CornerRadius = UDim.new(0, 7)
	
	local ControlStroke = Instance.new("UIStroke", ControlButtons)
	ControlStroke.Color = Color3.fromRGB(255, 0, 0)
	ControlStroke.Thickness = 1
	
	local ControlDivider = Instance.new("Frame", ControlButtons)
	ControlDivider.BorderSizePixel = 0
	ControlDivider.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
	ControlDivider.Size = UDim2.new(0, 1, 1, 0)
	ControlDivider.Position = UDim2.new(0.5, 0, 0, 0)
	
	-- Minimize button
	MinimizeBtn.Parent = ControlButtons
	MinimizeBtn.Size = UDim2.new(0.5, 0, 1, 0)
	MinimizeBtn.BackgroundTransparency = 1
	
	-- Close button
	CloseBtn.Parent = ControlButtons
	CloseBtn.Size = UDim2.new(0.5, 0, 1, 0)
	CloseBtn.Position = UDim2.new(0.5, 0, 0, 0)
	CloseBtn.BackgroundTransparency = 1
	
	-- Set drag point
	DragPoint.Parent = HeaderFrame
	DragPoint.Size = UDim2.new(1, -100, 1, 0)
	
	-- Set parent
	WindowContainer.Parent = Nexus
	
	-- ==============================================
	-- FUNGSI WINDOW UTAMA
	-- ==============================================
	AddDraggingFunctionality(DragPoint, WindowContainer)

	AddConnection(CloseBtn.MouseButton1Up, function()
		WindowContainer.Visible = false
		UIHidden = true
		NexusUI:MakeNotification({
			Name = "Interface Hidden",
			Content = "Tap RightShift to reopen the interface",
			Time = 5
		})
		WindowConfig.CloseCallback()
	end)

	AddConnection(UserInputService.InputBegan, function(Input)
		if Input.KeyCode == Enum.KeyCode.RightShift and UIHidden then
			WindowContainer.Visible = true
		end
	end)

	AddConnection(MinimizeBtn.MouseButton1Up, function()
		if Minimized then
			TweenService:Create(WindowContainer, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Size = UDim2.new(0, 536, 0, 282)}):Play()
			MinimizeBtn.Ico.Image = "rbxassetid://7072719338"
			wait(.02)
			WindowContainer.ClipsDescendants = false
			ContentScroll.Visible = true
			TabScroll.Visible = true
			HorizontalLine.Visible = true
			VerticalLine.Visible = true
		else
			WindowContainer.ClipsDescendants = true
			HorizontalLine.Visible = false
			VerticalLine.Visible = false
			MinimizeBtn.Ico.Image = "rbxassetid://7072720870"

			TweenService:Create(WindowContainer, TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Size = UDim2.new(0, TitleLabel.TextBounds.X + 140, 0, 52)}):Play()
			wait(0.1)
			ContentScroll.Visible = false
			TabScroll.Visible = false
		end
		Minimized = not Minimized    
	end)

	-- Update TabHolder connection untuk canvas size
	AddConnection(TabList:GetPropertyChangedSignal("AbsoluteContentSize"), function()
		TabScroll.CanvasSize = UDim2.new(0, 0, 0, TabList.AbsoluteContentSize.Y + 16)
	end)

	-- Update ContentScroll connection untuk canvas size
	AddConnection(ContentScroll:GetPropertyChangedSignal("AbsoluteContentSize"), function()
		ContentScroll.CanvasSize = UDim2.new(0, 0, 0, ContentScroll.UIListLayout.AbsoluteContentSize.Y + 24)
	end)

	local function LoadSequence()
		WindowContainer.Visible = false
		
		-- ====== CONFIG ======
		local GUI_NAME = "NexusUI"
		local FULL_TEXT = "Nexus\nUI"
		local TYPE_SPEED = 0.05
		local SHOW_DELAY = 0.5
		local HIDE_DELAY = 1.5
		
		-- ====== CREATE GUI ======
		local ScreenGui = Instance.new("ScreenGui")
		ScreenGui.Name = GUI_NAME
		ScreenGui.Parent = Nexus
		
		-- FRAME
		local Frame = Instance.new("Frame", ScreenGui)
		Frame.Name = "FrameAnimasi"
		Frame.Size = UDim2.new(0,150,0,44)
		Frame.Position = UDim2.new(0.5,-75,0.4,0)
		Frame.BackgroundColor3 = Color3.fromRGB(20,20,20)
		Frame.BorderSizePixel = 0
		Frame.BackgroundTransparency = 1
		
		-- Add corner
		local UICorner = Instance.new("UICorner", Frame)
		UICorner.CornerRadius = UDim.new(0, 8)
		
		-- GRADIENT
		local Gradient = Instance.new("UIGradient", Frame)
		Gradient.Color = ColorSequence.new{
			ColorSequenceKeypoint.new(0, Color3.fromRGB(30,30,30)),
			ColorSequenceKeypoint.new(1, Color3.fromRGB(10,10,10))
		}
		
		-- STROKE
		local Stroke = Instance.new("UIStroke", Frame)
		Stroke.Color = Color3.fromRGB(255,60,60)
		Stroke.Thickness = 2
		Stroke.Transparency = 1
		
		-- ICON
		local Image = Instance.new("ImageLabel", Frame)
		Image.Size = UDim2.new(0,34,0,34)
		Image.Position = UDim2.new(0,6,0.5,-17)
		Image.Image = "rbxassetid://82528573041751"
		Image.BackgroundTransparency = 1
		Image.ImageTransparency = 1
		
		local ImageCorner = Instance.new("UICorner", Image)
		ImageCorner.CornerRadius = UDim.new(0, 6)
		
		-- LINE
		local Line = Instance.new("Frame", Frame)
		Line.Size = UDim2.new(0,2,1,-8)
		Line.Position = UDim2.new(0,48,0,4)
		Line.BackgroundColor3 = Color3.fromRGB(255,60,60)
		Line.BorderSizePixel = 0
		Line.BackgroundTransparency = 1
		
		-- TEXT
		local Text = Instance.new("TextLabel", Frame)
		Text.Position = UDim2.new(0,54,0,4)
		Text.Size = UDim2.new(1,-58,1,-8)
		Text.BackgroundTransparency = 1
		Text.TextColor3 = Color3.fromRGB(255,255,255)
		Text.Font = Enum.Font.GothamBold
		Text.TextSize = 16
		Text.TextWrapped = true
		Text.TextXAlignment = Enum.TextXAlignment.Center
		Text.TextYAlignment = Enum.TextYAlignment.Center
		Text.TextTransparency = 1
		Text.Text = ""
		
		-- posisi awal
		Frame.Position = Frame.Position + UDim2.new(0,-50,0,0)
		
		local InInfo = TweenInfo.new(0.6, Enum.EasingStyle.Quint, Enum.EasingDirection.Out)
		local OutInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quad, Enum.EasingDirection.In)
		
		local function tween(obj, info, props)
			TweenService:Create(obj, info, props):Play()
		end
		
		local function typeText(label, text, speed)
			label.Text = ""
			label.TextTransparency = 0
			for i = 1, #text do
				label.Text = string.sub(text,1,i)
				task.wait(speed)
			end
		end
		
		task.wait(SHOW_DELAY)
		
		Frame.Visible = true
		tween(Frame, InInfo, {
			BackgroundTransparency = 0,
			Position = Frame.Position + UDim2.new(0,50,0,0)
		})
		
		tween(Stroke, InInfo, {Transparency = 0})
		tween(Image, InInfo, {ImageTransparency = 0})
		tween(Line, InInfo, {BackgroundTransparency = 0})
		
		task.wait(0.5)
		typeText(Text, FULL_TEXT, TYPE_SPEED)
		
		task.wait(HIDE_DELAY)
		
		tween(Text, OutInfo, {TextTransparency = 1})
		tween(Image, OutInfo, {ImageTransparency = 1})
		tween(Line, OutInfo, {BackgroundTransparency = 1})
		tween(Stroke, OutInfo, {Transparency = 1})
		tween(Frame, OutInfo, {
			BackgroundTransparency = 1,
			Position = Frame.Position + UDim2.new(0,30,0,0)
		})
		
		task.wait(0.6)
		if ScreenGui then
			ScreenGui:Destroy()
		end
		
		WindowContainer.Visible = true
	end

	if WindowConfig.IntroEnabled then
		LoadSequence()
	end	

	local TabFunction = {}
	function TabFunction:MakeTab(TabConfig)
		TabConfig = TabConfig or {}
		TabConfig.Name = TabConfig.Name or "Tab"
		TabConfig.Icon = TabConfig.Icon or ""
		TabConfig.PremiumOnly = TabConfig.PremiumOnly or false

		-- Create tab button dengan desain merah-hitam
		local TabButtonFrame = Instance.new("Frame", TabScroll)
		TabButtonFrame.BorderSizePixel = 0
		TabButtonFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
		TabButtonFrame.Size = UDim2.new(1, -16, 0, 32)
		TabButtonFrame.BackgroundTransparency = 0.7
		
		local TabButtonCorner = Instance.new("UICorner", TabButtonFrame)
		TabButtonCorner.CornerRadius = UDim.new(0, 6)
		
		local TabButtonStroke = Instance.new("UIStroke", TabButtonFrame)
		TabButtonStroke.Color = Color3.fromRGB(255, 0, 0)
		TabButtonStroke.Thickness = 1
		
		-- Icon jika ada
		if TabConfig.Icon ~= "" then
			local TabIcon = Instance.new("ImageLabel", TabButtonFrame)
			TabIcon.Size = UDim2.new(0, 18, 0, 18)
			TabIcon.Position = UDim2.new(0, 10, 0.5, -9)
			TabIcon.Image = TabConfig.Icon
			TabIcon.BackgroundTransparency = 1
			TabIcon.ImageTransparency = 0.4
			TabIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
		end
		
		-- Text label
		local TabLabel = Instance.new("TextLabel", TabButtonFrame)
		if TabConfig.Icon ~= "" then
			TabLabel.Size = UDim2.new(1, -35, 1, 0)
			TabLabel.Position = UDim2.new(0, 35, 0, 0)
		else
			TabLabel.Size = UDim2.new(1, -12, 1, 0)
			TabLabel.Position = UDim2.new(0, 12, 0, 0)
		end
		TabLabel.BackgroundTransparency = 1
		TabLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
		TabLabel.Font = Enum.Font.GothamSemibold
		TabLabel.TextSize = 14
		TabLabel.Text = TabConfig.Name
		TabLabel.TextTransparency = 0.4
		TabLabel.TextXAlignment = Enum.TextXAlignment.Left
		
		-- Button untuk click
		local TabButton = Instance.new("TextButton", TabButtonFrame)
		TabButton.Size = UDim2.new(1, 0, 1, 0)
		TabButton.BackgroundTransparency = 1
		TabButton.Text = ""
		
		-- Store tab for search functionality
		TabButtons[TabConfig.Name] = TabButtonFrame

		-- Create content container untuk tab ini
		local Container = Instance.new("ScrollingFrame", ContentScroll)
		Container.BorderSizePixel = 0
		Container.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Container.Size = UDim2.new(1, 0, 1, 0)
		Container.ScrollBarImageColor3 = Color3.fromRGB(255, 0, 0)
		Container.ScrollBarThickness = 5
		Container.BackgroundTransparency = 1
		Container.Visible = false
		Container.CanvasSize = UDim2.new(0, 0, 0, 0)
		
		local ContainerList = Instance.new("UIListLayout", Container)
		ContainerList.SortOrder = Enum.SortOrder.LayoutOrder
		ContainerList.Padding = UDim.new(0, 6)
		
		local ContainerPadding = Instance.new("UIPadding", Container)
		ContainerPadding.PaddingLeft = UDim.new(0, 0)
		ContainerPadding.PaddingRight = UDim.new(0, 0)
		ContainerPadding.PaddingTop = UDim.new(0, 0)
		ContainerPadding.PaddingBottom = UDim.new(0, 0)

		AddConnection(ContainerList:GetPropertyChangedSignal("AbsoluteContentSize"), function()
			Container.CanvasSize = UDim2.new(0, 0, 0, ContainerList.AbsoluteContentSize.Y + 30)
		end)

		if FirstTab then
			FirstTab = false
			if TabConfig.Icon ~= "" then
				TabButtonFrame:FindFirstChildWhichIsA("ImageLabel").ImageTransparency = 0
			end
			TabLabel.TextTransparency = 0
			TabLabel.Font = Enum.Font.GothamBlack
			Container.Visible = true
		end    

		AddConnection(TabButton.MouseButton1Click, function()
			-- Reset semua tab
			for _, Tab in next, TabScroll:GetChildren() do
				if Tab:IsA("Frame") then
					local label = Tab:FindFirstChildWhichIsA("TextLabel")
					if label then
						label.Font = Enum.Font.GothamSemibold
						label.TextTransparency = 0.4
					end
					local icon = Tab:FindFirstChildWhichIsA("ImageLabel")
					if icon then
						TweenService:Create(icon, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 0.4}):Play()
					end
					TweenService:Create(Tab, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.7}):Play()
				end    
			end
			
			-- Sembunyikan semua container
			for _, ItemContainer in next, ContentScroll:GetChildren() do
				if ItemContainer:IsA("ScrollingFrame") then
					ItemContainer.Visible = false
				end    
			end  
			
			-- Highlight tab yang dipilih
			if TabConfig.Icon ~= "" then
				TweenService:Create(TabButtonFrame:FindFirstChildWhichIsA("ImageLabel"), TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 0}):Play()
			end
			TweenService:Create(TabLabel, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
			TweenService:Create(TabButtonFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
			TabLabel.Font = Enum.Font.GothamBlack
			Container.Visible = true   
		end)

		local function GetElements(ItemParent)
			local ElementFunction = {}
			function ElementFunction:AddLabel(Text)
				local LabelFrame = Instance.new("Frame")
				LabelFrame.Parent = ItemParent
				LabelFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				LabelFrame.BorderSizePixel = 0
				LabelFrame.Size = UDim2.new(1, 0, 0, 32)
				LabelFrame.BackgroundTransparency = 0.7
				
				local LabelCorner = Instance.new("UICorner", LabelFrame)
				LabelCorner.CornerRadius = UDim.new(0, 6)
				
				local LabelStroke = Instance.new("UIStroke", LabelFrame)
				LabelStroke.Color = Color3.fromRGB(255, 0, 0)
				LabelStroke.Thickness = 1
				
				local LabelText = Instance.new("TextLabel", LabelFrame)
				LabelText.Size = UDim2.new(1, -12, 1, 0)
				LabelText.Position = UDim2.new(0, 12, 0, 0)
				LabelText.BackgroundTransparency = 1
				LabelText.TextColor3 = Color3.fromRGB(255, 255, 255)
				LabelText.Font = Enum.Font.GothamBold
				LabelText.TextSize = 15
				LabelText.Text = Text
				LabelText.TextXAlignment = Enum.TextXAlignment.Left
				
				local LabelFunc = {}
				function LabelFunc:Set(ToChange)
					LabelText.Text = ToChange
				end
				return LabelFunc
			end
			
			function ElementFunction:AddParagraph(Text, Content)
				Text = Text or "Text"
				Content = Content or "Content"

				local ParagraphFrame = Instance.new("Frame")
				ParagraphFrame.Parent = ItemParent
				ParagraphFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				ParagraphFrame.BorderSizePixel = 0
				ParagraphFrame.Size = UDim2.new(1, 0, 0, 32)
				ParagraphFrame.BackgroundTransparency = 0.7
				ParagraphFrame.AutomaticSize = Enum.AutomaticSize.Y
				
				local ParagraphCorner = Instance.new("UICorner", ParagraphFrame)
				ParagraphCorner.CornerRadius = UDim.new(0, 6)
				
				local ParagraphStroke = Instance.new("UIStroke", ParagraphFrame)
				ParagraphStroke.Color = Color3.fromRGB(255, 0, 0)
				ParagraphStroke.Thickness = 1
				
				local TitleLabel = Instance.new("TextLabel", ParagraphFrame)
				TitleLabel.Size = UDim2.new(1, -12, 0, 14)
				TitleLabel.Position = UDim2.new(0, 12, 0, 10)
				TitleLabel.BackgroundTransparency = 1
				TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				TitleLabel.Font = Enum.Font.GothamBold
				TitleLabel.TextSize = 15
				TitleLabel.Text = Text
				TitleLabel.TextXAlignment = Enum.TextXAlignment.Left
				
				local ContentLabel = Instance.new("TextLabel", ParagraphFrame)
				ContentLabel.Size = UDim2.new(1, -24, 0, 0)
				ContentLabel.Position = UDim2.new(0, 12, 0, 26)
				ContentLabel.BackgroundTransparency = 1
				ContentLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
				ContentLabel.Font = Enum.Font.GothamSemibold
				ContentLabel.TextSize = 13
				ContentLabel.Text = Content
				ContentLabel.TextXAlignment = Enum.TextXAlignment.Left
				ContentLabel.TextWrapped = true
				ContentLabel.AutomaticSize = Enum.AutomaticSize.Y
				
				ContentLabel:GetPropertyChangedSignal("Text"):Connect(function()
					ParagraphFrame.Size = UDim2.new(1, 0, 0, ContentLabel.TextBounds.Y + 35)
				end)

				local ParagraphFunction = {}
				function ParagraphFunction:Set(ToChange)
					ContentLabel.Text = ToChange
				end
				return ParagraphFunction
			end    
			
			function ElementFunction:AddButton(ButtonConfig)
				ButtonConfig = ButtonConfig or {}
				ButtonConfig.Name = ButtonConfig.Name or "Button"
				ButtonConfig.Callback = ButtonConfig.Callback or function() end
				ButtonConfig.Icon = ButtonConfig.Icon or "rbxassetid://3944703587"

				local Button = {}

				local ButtonFrame = Instance.new("Frame")
				ButtonFrame.Parent = ItemParent
				ButtonFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				ButtonFrame.BorderSizePixel = 0
				ButtonFrame.Size = UDim2.new(1, 0, 0, 35)
				ButtonFrame.BackgroundTransparency = 0.7
				
				local ButtonCorner = Instance.new("UICorner", ButtonFrame)
				ButtonCorner.CornerRadius = UDim.new(0, 6)
				
				local ButtonStroke = Instance.new("UIStroke", ButtonFrame)
				ButtonStroke.Color = Color3.fromRGB(255, 0, 0)
				ButtonStroke.Thickness = 1
				
				local ButtonText = Instance.new("TextLabel", ButtonFrame)
				ButtonText.Size = UDim2.new(1, -12, 1, 0)
				ButtonText.Position = UDim2.new(0, 12, 0, 0)
				ButtonText.BackgroundTransparency = 1
				ButtonText.TextColor3 = Color3.fromRGB(255, 255, 255)
				ButtonText.Font = Enum.Font.GothamBold
				ButtonText.TextSize = 15
				ButtonText.Text = ButtonConfig.Name
				ButtonText.TextXAlignment = Enum.TextXAlignment.Left
				
				if ButtonConfig.Icon ~= "" then
					local ButtonIcon = Instance.new("ImageLabel", ButtonFrame)
					ButtonIcon.Size = UDim2.new(0, 20, 0, 20)
					ButtonIcon.Position = UDim2.new(1, -30, 0, 8)
					ButtonIcon.Image = ButtonConfig.Icon
					ButtonIcon.BackgroundTransparency = 1
					ButtonIcon.ImageColor3 = Color3.fromRGB(200, 200, 200)
				end
				
				local ClickButton = Instance.new("TextButton", ButtonFrame)
				ClickButton.Size = UDim2.new(1, 0, 1, 0)
				ClickButton.BackgroundTransparency = 1
				ClickButton.Text = ""

				AddConnection(ClickButton.MouseEnter, function()
					TweenService:Create(ButtonFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
				end)

				AddConnection(ClickButton.MouseLeave, function()
					TweenService:Create(ButtonFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.7}):Play()
				end)

				AddConnection(ClickButton.MouseButton1Up, function()
					TweenService:Create(ButtonFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
					spawn(function()
						ButtonConfig.Callback()
					end)
				end)

				AddConnection(ClickButton.MouseButton1Down, function()
					TweenService:Create(ButtonFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.3}):Play()
				end)

				function Button:Set(ButtonText)
					ButtonText.Text = ButtonText
				end	

				return Button
			end
			
			function ElementFunction:AddToggle(ToggleConfig)
				ToggleConfig = ToggleConfig or {}
				ToggleConfig.Name = ToggleConfig.Name or "Toggle"
				ToggleConfig.Default = ToggleConfig.Default or false
				ToggleConfig.Callback = ToggleConfig.Callback or function() end
				ToggleConfig.Flag = ToggleConfig.Flag or nil
				ToggleConfig.Save = ToggleConfig.Save or false

				local Toggle = {Value = ToggleConfig.Default, Save = ToggleConfig.Save}

				local ToggleFrame = Instance.new("Frame")
				ToggleFrame.Parent = ItemParent
				ToggleFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				ToggleFrame.BorderSizePixel = 0
				ToggleFrame.Size = UDim2.new(1, 0, 0, 38)
				ToggleFrame.BackgroundTransparency = 0.7
				
				local ToggleCorner = Instance.new("UICorner", ToggleFrame)
				ToggleCorner.CornerRadius = UDim.new(0, 6)
				
				local ToggleStroke = Instance.new("UIStroke", ToggleFrame)
				ToggleStroke.Color = Color3.fromRGB(255, 0, 0)
				ToggleStroke.Thickness = 1
				
				local ToggleText = Instance.new("TextLabel", ToggleFrame)
				ToggleText.Size = UDim2.new(1, -12, 1, 0)
				ToggleText.Position = UDim2.new(0, 12, 0, 0)
				ToggleText.BackgroundTransparency = 1
				ToggleText.TextColor3 = Color3.fromRGB(255, 255, 255)
				ToggleText.Font = Enum.Font.GothamBold
				ToggleText.TextSize = 15
				ToggleText.Text = ToggleConfig.Name
				ToggleText.TextXAlignment = Enum.TextXAlignment.Left
				
				-- Toggle box
				local ToggleBox = Instance.new("Frame", ToggleFrame)
				ToggleBox.BackgroundColor3 = Toggle.Value and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(0, 0, 0)
				ToggleBox.BorderSizePixel = 0
				ToggleBox.Size = UDim2.new(0, 24, 0, 24)
				ToggleBox.Position = UDim2.new(1, -24, 0.5, -12)
				
				local BoxCorner = Instance.new("UICorner", ToggleBox)
				BoxCorner.CornerRadius = UDim.new(0, 4)
				
				local BoxStroke = Instance.new("UIStroke", ToggleBox)
				BoxStroke.Color = Toggle.Value and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(255, 0, 0)
				BoxStroke.Thickness = 1
				BoxStroke.Transparency = 0.5
				
				local CheckIcon = Instance.new("ImageLabel", ToggleBox)
				CheckIcon.Size = UDim2.new(0, 20, 0, 20)
				CheckIcon.Position = UDim2.new(0.5, -10, 0.5, -10)
				CheckIcon.Image = "rbxassetid://3944680095"
				CheckIcon.BackgroundTransparency = 1
				CheckIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
				CheckIcon.ImageTransparency = Toggle.Value and 0 or 1
				
				local ClickButton = Instance.new("TextButton", ToggleFrame)
				ClickButton.Size = UDim2.new(1, 0, 1, 0)
				ClickButton.BackgroundTransparency = 1
				ClickButton.Text = ""

				function Toggle:Set(Value)
					self.Value = Value
					TweenService:Create(ToggleBox, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundColor3 = self.Value and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(0, 0, 0)}):Play()
					TweenService:Create(BoxStroke, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Color = self.Value and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(255, 0, 0)}):Play()
					TweenService:Create(CheckIcon, TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = self.Value and 0 or 1}):Play()
					ToggleConfig.Callback(self.Value)
				end    

				Toggle:Set(Toggle.Value)

				AddConnection(ClickButton.MouseEnter, function()
					TweenService:Create(ToggleFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
				end)

				AddConnection(ClickButton.MouseLeave, function()
					TweenService:Create(ToggleFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.7}):Play()
				end)

				AddConnection(ClickButton.MouseButton1Up, function()
					TweenService:Create(ToggleFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
					SaveCfg(game.GameId)
					Toggle:Set(not Toggle.Value)
				end)

				AddConnection(ClickButton.MouseButton1Down, function()
					TweenService:Create(ToggleFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.3}):Play()
				end)

				if ToggleConfig.Flag then
					NexusUI.Flags[ToggleConfig.Flag] = Toggle
				end	
				return Toggle
			end
			
			function ElementFunction:AddSlider(SliderConfig)
				SliderConfig = SliderConfig or {}
				SliderConfig.Name = SliderConfig.Name or "Slider"
				SliderConfig.Min = SliderConfig.Min or 0
				SliderConfig.Max = SliderConfig.Max or 100
				SliderConfig.Increment = SliderConfig.Increment or 1
				SliderConfig.Default = SliderConfig.Default or 50
				SliderConfig.Callback = SliderConfig.Callback or function() end
				SliderConfig.ValueName = SliderConfig.ValueName or ""
				SliderConfig.Flag = SliderConfig.Flag or nil
				SliderConfig.Save = SliderConfig.Save or false

				local Slider = {Value = SliderConfig.Default, Save = SliderConfig.Save}
				local Dragging = false

				local SliderFrame = Instance.new("Frame")
				SliderFrame.Parent = ItemParent
				SliderFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				SliderFrame.BorderSizePixel = 0
				SliderFrame.Size = UDim2.new(1, 0, 0, 65)
				SliderFrame.BackgroundTransparency = 0.7
				
				local SliderCorner = Instance.new("UICorner", SliderFrame)
				SliderCorner.CornerRadius = UDim.new(0, 6)
				
				local SliderStroke = Instance.new("UIStroke", SliderFrame)
				SliderStroke.Color = Color3.fromRGB(255, 0, 0)
				SliderStroke.Thickness = 1
				
				local SliderText = Instance.new("TextLabel", SliderFrame)
				SliderText.Size = UDim2.new(1, -12, 0, 14)
				SliderText.Position = UDim2.new(0, 12, 0, 10)
				SliderText.BackgroundTransparency = 1
				SliderText.TextColor3 = Color3.fromRGB(255, 255, 255)
				SliderText.Font = Enum.Font.GothamBold
				SliderText.TextSize = 15
				SliderText.Text = SliderConfig.Name
				SliderText.TextXAlignment = Enum.TextXAlignment.Left
				
				local SliderBar = Instance.new("Frame", SliderFrame)
				SliderBar.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				SliderBar.BorderSizePixel = 0
				SliderBar.Size = UDim2.new(1, -24, 0, 26)
				SliderBar.Position = UDim2.new(0, 12, 0, 30)
				SliderBar.BackgroundTransparency = 0.9
				
				local BarCorner = Instance.new("UICorner", SliderBar)
				BarCorner.CornerRadius = UDim.new(0, 5)
				
				local BarStroke = Instance.new("UIStroke", SliderBar)
				BarStroke.Color = Color3.fromRGB(255, 0, 0)
				BarStroke.Thickness = 1
				
				local SliderDrag = Instance.new("Frame", SliderBar)
				SliderDrag.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				SliderDrag.BorderSizePixel = 0
				SliderDrag.Size = UDim2.new(0, 0, 1, 0)
				SliderDrag.BackgroundTransparency = 0.3
				
				local DragCorner = Instance.new("UICorner", SliderDrag)
				DragCorner.CornerRadius = UDim.new(0, 5)
				
				local ValueLabel = Instance.new("TextLabel", SliderBar)
				ValueLabel.Size = UDim2.new(1, -12, 0, 14)
				ValueLabel.Position = UDim2.new(0, 12, 0, 6)
				ValueLabel.BackgroundTransparency = 1
				ValueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				ValueLabel.Font = Enum.Font.GothamBold
				ValueLabel.TextSize = 13
				ValueLabel.Text = tostring(Slider.Value) .. " " .. SliderConfig.ValueName
				ValueLabel.TextXAlignment = Enum.TextXAlignment.Left
				ValueLabel.TextTransparency = 0.8
				
				local DragValueLabel = Instance.new("TextLabel", SliderDrag)
				DragValueLabel.Size = UDim2.new(1, -12, 0, 14)
				DragValueLabel.Position = UDim2.new(0, 12, 0, 6)
				DragValueLabel.BackgroundTransparency = 1
				DragValueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				DragValueLabel.Font = Enum.Font.GothamBold
				DragValueLabel.TextSize = 13
				DragValueLabel.Text = tostring(Slider.Value) .. " " .. SliderConfig.ValueName
				DragValueLabel.TextXAlignment = Enum.TextXAlignment.Left
				DragValueLabel.TextTransparency = 0

				SliderBar.InputBegan:Connect(function(Input)
					if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then 
						Dragging = true 
					end 
				end)
				
				SliderBar.InputEnded:Connect(function(Input) 
					if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then 
						Dragging = false 
					end 
				end)

				UserInputService.InputChanged:Connect(function(Input)
					if Dragging then 
						local SizeScale = math.clamp((Input.Position.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1)
						Slider:Set(SliderConfig.Min + ((SliderConfig.Max - SliderConfig.Min) * SizeScale)) 
						SaveCfg(game.GameId)
					end
				end)

				function Slider:Set(Value)
					self.Value = math.clamp(Round(Value, SliderConfig.Increment), SliderConfig.Min, SliderConfig.Max)
					TweenService:Create(SliderDrag, TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.fromScale((self.Value - SliderConfig.Min) / (SliderConfig.Max - SliderConfig.Min), 1)}):Play()
					ValueLabel.Text = tostring(self.Value) .. " " .. SliderConfig.ValueName
					DragValueLabel.Text = tostring(self.Value) .. " " .. SliderConfig.ValueName
					SliderConfig.Callback(self.Value)
				end      

				Slider:Set(Slider.Value)
				if SliderConfig.Flag then				
					NexusUI.Flags[SliderConfig.Flag] = Slider
				end
				return Slider
			end
			
			function ElementFunction:AddDropdown(DropdownConfig)
				DropdownConfig = DropdownConfig or {}
				DropdownConfig.Name = DropdownConfig.Name or "Dropdown"
				DropdownConfig.Options = DropdownConfig.Options or {}
				DropdownConfig.Default = DropdownConfig.Default or ""
				DropdownConfig.Callback = DropdownConfig.Callback or function() end
				DropdownConfig.Flag = DropdownConfig.Flag or nil
				DropdownConfig.Save = DropdownConfig.Save or false

				local Dropdown = {Value = DropdownConfig.Default, Options = DropdownConfig.Options, Buttons = {}, Toggled = false, Type = "Dropdown", Save = DropdownConfig.Save}
				local MaxElements = 5

				if not table.find(Dropdown.Options, Dropdown.Value) then
					Dropdown.Value = "..."
				end

				local DropdownFrame = Instance.new("Frame")
				DropdownFrame.Parent = ItemParent
				DropdownFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				DropdownFrame.BorderSizePixel = 0
				DropdownFrame.Size = UDim2.new(1, 0, 0, 38)
				DropdownFrame.BackgroundTransparency = 0.7
				DropdownFrame.ClipsDescendants = true
				
				local DropdownCorner = Instance.new("UICorner", DropdownFrame)
				DropdownCorner.CornerRadius = UDim.new(0, 6)
				
				local DropdownStroke = Instance.new("UIStroke", DropdownFrame)
				DropdownStroke.Color = Color3.fromRGB(255, 0, 0)
				DropdownStroke.Thickness = 1
				
				local DropdownText = Instance.new("TextLabel", DropdownFrame)
				DropdownText.Size = UDim2.new(1, -12, 1, 0)
				DropdownText.Position = UDim2.new(0, 12, 0, 0)
				DropdownText.BackgroundTransparency = 1
				DropdownText.TextColor3 = Color3.fromRGB(255, 255, 255)
				DropdownText.Font = Enum.Font.GothamBold
				DropdownText.TextSize = 15
				DropdownText.Text = DropdownConfig.Name
				DropdownText.TextXAlignment = Enum.TextXAlignment.Left
				
				local SelectedText = Instance.new("TextLabel", DropdownFrame)
				SelectedText.Size = UDim2.new(1, -40, 1, 0)
				SelectedText.Position = UDim2.new(0, 0, 0, 0)
				SelectedText.BackgroundTransparency = 1
				SelectedText.TextColor3 = Color3.fromRGB(200, 200, 200)
				SelectedText.Font = Enum.Font.Gotham
				SelectedText.TextSize = 13
				SelectedText.Text = Dropdown.Value
				SelectedText.TextXAlignment = Enum.TextXAlignment.Right
				
				local ArrowIcon = Instance.new("ImageLabel", DropdownFrame)
				ArrowIcon.Size = UDim2.new(0, 20, 0, 20)
				ArrowIcon.Position = UDim2.new(1, -30, 0.5, -10)
				ArrowIcon.Image = "rbxassetid://7072706796"
				ArrowIcon.BackgroundTransparency = 1
				ArrowIcon.ImageColor3 = Color3.fromRGB(200, 200, 200)
				
				local DividerLine = Instance.new("Frame", DropdownFrame)
				DividerLine.BorderSizePixel = 0
				DividerLine.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
				DividerLine.Size = UDim2.new(1, 0, 0, 1)
				DividerLine.Position = UDim2.new(0, 0, 1, -1)
				DividerLine.Visible = false
				
				local DropdownContainer = Instance.new("ScrollingFrame", DropdownFrame)
				DropdownContainer.BorderSizePixel = 0
				DropdownContainer.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				DropdownContainer.Size = UDim2.new(1, 0, 1, -38)
				DropdownContainer.Position = UDim2.new(0, 0, 0, 38)
				DropdownContainer.ScrollBarImageColor3 = Color3.fromRGB(255, 0, 0)
				DropdownContainer.ScrollBarThickness = 4
				DropdownContainer.BackgroundTransparency = 1
				DropdownContainer.CanvasSize = UDim2.new(0, 0, 0, 0)
				DropdownContainer.Visible = false
				
				local DropdownList = Instance.new("UIListLayout", DropdownContainer)
				DropdownList.SortOrder = Enum.SortOrder.LayoutOrder
				DropdownList.Padding = UDim.new(0, 4)
				
				local ClickButton = Instance.new("TextButton", DropdownFrame)
				ClickButton.Size = UDim2.new(1, 0, 0, 38)
				ClickButton.BackgroundTransparency = 1
				ClickButton.Text = ""

				AddConnection(DropdownList:GetPropertyChangedSignal("AbsoluteContentSize"), function()
					DropdownContainer.CanvasSize = UDim2.new(0, 0, 0, DropdownList.AbsoluteContentSize.Y)
				end)  

				local function AddOptions(Options)
					for _, Option in pairs(Options) do
						local OptionBtn = Instance.new("TextButton", DropdownContainer)
						OptionBtn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
						OptionBtn.BorderSizePixel = 0
						OptionBtn.Size = UDim2.new(1, 0, 0, 28)
						OptionBtn.BackgroundTransparency = 1
						OptionBtn.Text = ""
						
						local BtnCorner = Instance.new("UICorner", OptionBtn)
						BtnCorner.CornerRadius = UDim.new(0, 6)
						
						local BtnStroke = Instance.new("UIStroke", OptionBtn)
						BtnStroke.Color = Color3.fromRGB(255, 0, 0)
						BtnStroke.Thickness = 1
						
						local BtnText = Instance.new("TextLabel", OptionBtn)
						BtnText.Size = UDim2.new(1, -8, 1, 0)
						BtnText.Position = UDim2.new(0, 8, 0, 0)
						BtnText.BackgroundTransparency = 1
						BtnText.TextColor3 = Color3.fromRGB(255, 255, 255)
						BtnText.Font = Enum.Font.Gotham
						BtnText.TextSize = 13
						BtnText.Text = Option
						BtnText.TextXAlignment = Enum.TextXAlignment.Left
						BtnText.TextTransparency = 0.4

						AddConnection(OptionBtn.MouseButton1Click, function()
							Dropdown:Set(Option)
							SaveCfg(game.GameId)
						end)

						Dropdown.Buttons[Option] = OptionBtn
					end
				end	

				function Dropdown:Refresh(Options, Delete)
					if Delete then
						for _,v in pairs(Dropdown.Buttons) do
							v:Destroy()
						end    
						table.clear(Dropdown.Options)
						table.clear(Dropdown.Buttons)
					end
					Dropdown.Options = Options
					AddOptions(Dropdown.Options)
				end  

				function Dropdown:Set(Value)
					if not table.find(Dropdown.Options, Value) then
						Dropdown.Value = "..."
						SelectedText.Text = Dropdown.Value
						for _, v in pairs(Dropdown.Buttons) do
							TweenService:Create(v,TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
							TweenService:Create(v:FindFirstChildWhichIsA("TextLabel"),TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
						end	
						return
					end

					Dropdown.Value = Value
					SelectedText.Text = Dropdown.Value

					for _, v in pairs(Dropdown.Buttons) do
						TweenService:Create(v,TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
						TweenService:Create(v:FindFirstChildWhichIsA("TextLabel"),TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.4}):Play()
					end	
					
					if Dropdown.Buttons[Value] then
						TweenService:Create(Dropdown.Buttons[Value],TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
						TweenService:Create(Dropdown.Buttons[Value]:FindFirstChildWhichIsA("TextLabel"),TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
					end
					
					return DropdownConfig.Callback(Dropdown.Value)
				end

				AddConnection(ClickButton.MouseButton1Click, function()
					Dropdown.Toggled = not Dropdown.Toggled
					DividerLine.Visible = Dropdown.Toggled
					DropdownContainer.Visible = Dropdown.Toggled
					
					TweenService:Create(ArrowIcon,TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Rotation = Dropdown.Toggled and 180 or 0}):Play()
					if #Dropdown.Options > MaxElements then
						TweenService:Create(DropdownFrame,TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Size = Dropdown.Toggled and UDim2.new(1, 0, 0, 38 + (MaxElements * 28)) or UDim2.new(1, 0, 0, 38)}):Play()
					else
						TweenService:Create(DropdownFrame,TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Size = Dropdown.Toggled and UDim2.new(1, 0, 0, DropdownList.AbsoluteContentSize.Y + 38) or UDim2.new(1, 0, 0, 38)}):Play()
					end
				end)

				Dropdown:Refresh(Dropdown.Options, false)
				Dropdown:Set(Dropdown.Value)
				if DropdownConfig.Flag then				
					OrionLib.Flags[DropdownConfig.Flag] = Dropdown
				end
				return Dropdown
			end
			
			function ElementFunction:AddBind(BindConfig)
				BindConfig.Name = BindConfig.Name or "Bind"
				BindConfig.Default = BindConfig.Default or Enum.KeyCode.Unknown
				BindConfig.Hold = BindConfig.Hold or false
				BindConfig.Callback = BindConfig.Callback or function() end
				BindConfig.Flag = BindConfig.Flag or nil
				BindConfig.Save = BindConfig.Save or false

				local Bind = {Value, Binding = false, Type = "Bind", Save = BindConfig.Save}
				local Holding = false

				local BindFrame = Instance.new("Frame")
				BindFrame.Parent = ItemParent
				BindFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				BindFrame.BorderSizePixel = 0
				BindFrame.Size = UDim2.new(1, 0, 0, 38)
				BindFrame.BackgroundTransparency = 0.7
				
				local BindCorner = Instance.new("UICorner", BindFrame)
				BindCorner.CornerRadius = UDim.new(0, 6)
				
				local BindStroke = Instance.new("UIStroke", BindFrame)
				BindStroke.Color = Color3.fromRGB(255, 0, 0)
				BindStroke.Thickness = 1
				
				local BindText = Instance.new("TextLabel", BindFrame)
				BindText.Size = UDim2.new(1, -12, 1, 0)
				BindText.Position = UDim2.new(0, 12, 0, 0)
				BindText.BackgroundTransparency = 1
				BindText.TextColor3 = Color3.fromRGB(255, 255, 255)
				BindText.Font = Enum.Font.GothamBold
				BindText.TextSize = 15
				BindText.Text = BindConfig.Name
				BindText.TextXAlignment = Enum.TextXAlignment.Left
				
				local BindBox = Instance.new("Frame", BindFrame)
				BindBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				BindBox.BorderSizePixel = 0
				BindBox.Size = UDim2.new(0, 24, 0, 24)
				BindBox.Position = UDim2.new(1, -12, 0.5, -12)
				BindBox.AnchorPoint = Vector2.new(1, 0.5)
				
				local BoxCorner = Instance.new("UICorner", BindBox)
				BoxCorner.CornerRadius = UDim.new(0, 6)
				
				local BoxStroke = Instance.new("UIStroke", BindBox)
				BoxStroke.Color = Color3.fromRGB(255, 0, 0)
				BoxStroke.Thickness = 1
				
				local ValueLabel = Instance.new("TextLabel", BindBox)
				ValueLabel.Size = UDim2.new(1, 0, 1, 0)
				ValueLabel.BackgroundTransparency = 1
				ValueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				ValueLabel.Font = Enum.Font.GothamBold
				ValueLabel.TextSize = 14
				ValueLabel.Text = BindConfig.Default.Name or BindConfig.Default
				ValueLabel.TextXAlignment = Enum.TextXAlignment.Center
				
				local ClickButton = Instance.new("TextButton", BindFrame)
				ClickButton.Size = UDim2.new(1, 0, 1, 0)
				ClickButton.BackgroundTransparency = 1
				ClickButton.Text = ""

				AddConnection(ValueLabel:GetPropertyChangedSignal("Text"), function()
					BindBox.Size = UDim2.new(0, ValueLabel.TextBounds.X + 16, 0, 24)
				end)

				AddConnection(ClickButton.InputEnded, function(Input)
					if Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch then
						if Bind.Binding then return end
						Bind.Binding = true
						ValueLabel.Text = "..."
					end
				end)

				AddConnection(UserInputService.InputBegan, function(Input)
					if UserInputService:GetFocusedTextBox() then return end
					if (Input.KeyCode.Name == Bind.Value or Input.UserInputType.Name == Bind.Value) and not Bind.Binding then
						if BindConfig.Hold then
							Holding = true
							BindConfig.Callback(Holding)
						else
							BindConfig.Callback()
						end
					elseif Bind.Binding then
						local Key
						pcall(function()
							if not CheckKey(BlacklistedKeys, Input.KeyCode) then
								Key = Input.KeyCode
							end
						end)
						pcall(function()
							if CheckKey(WhitelistedMouse, Input.UserInputType) and not Key then
								Key = Input.UserInputType
							end
						end)
						Key = Key or Bind.Value
						Bind:Set(Key)
						SaveCfg(game.GameId)
					end
				end)

				AddConnection(UserInputService.InputEnded, function(Input)
					if Input.KeyCode.Name == Bind.Value or Input.UserInputType.Name == Bind.Value then
						if BindConfig.Hold and Holding then
							Holding = false
							BindConfig.Callback(Holding)
						end
					end
				end)

				AddConnection(ClickButton.MouseEnter, function()
					TweenService:Create(BindFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
				end)

				AddConnection(ClickButton.MouseLeave, function()
					TweenService:Create(BindFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.7}):Play()
				end)

				function Bind:Set(Key)
					Bind.Binding = false
					Bind.Value = Key or Bind.Value
					Bind.Value = Bind.Value.Name or Bind.Value
					ValueLabel.Text = Bind.Value
				end

				Bind:Set(BindConfig.Default)
				if BindConfig.Flag then				
					NexusUI.Flags[BindConfig.Flag] = Bind
				end
				return Bind
			end
			
			function ElementFunction:AddTextbox(TextboxConfig)
				TextboxConfig = TextboxConfig or {}
				TextboxConfig.Name = TextboxConfig.Name or "Textbox"
				TextboxConfig.Default = TextboxConfig.Default or ""
				TextboxConfig.TextDisappear = TextboxConfig.TextDisappear or false
				TextboxConfig.Callback = TextboxConfig.Callback or function() end

				local TextboxFrame = Instance.new("Frame")
				TextboxFrame.Parent = ItemParent
				TextboxFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				TextboxFrame.BorderSizePixel = 0
				TextboxFrame.Size = UDim2.new(1, 0, 0, 38)
				TextboxFrame.BackgroundTransparency = 0.7
				
				local TextboxCorner = Instance.new("UICorner", TextboxFrame)
				TextboxCorner.CornerRadius = UDim.new(0, 6)
				
				local TextboxStroke = Instance.new("UIStroke", TextboxFrame)
				TextboxStroke.Color = Color3.fromRGB(255, 0, 0)
				TextboxStroke.Thickness = 1
				
				local TextboxText = Instance.new("TextLabel", TextboxFrame)
				TextboxText.Size = UDim2.new(1, -12, 1, 0)
				TextboxText.Position = UDim2.new(0, 12, 0, 0)
				TextboxText.BackgroundTransparency = 1
				TextboxText.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextboxText.Font = Enum.Font.GothamBold
				TextboxText.TextSize = 15
				TextboxText.Text = TextboxConfig.Name
				TextboxText.TextXAlignment = Enum.TextXAlignment.Left
				
				local InputBox = Instance.new("Frame", TextboxFrame)
				InputBox.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				InputBox.BorderSizePixel = 0
				InputBox.Size = UDim2.new(0, 24, 0, 24)
				InputBox.Position = UDim2.new(1, -12, 0.5, -12)
				InputBox.AnchorPoint = Vector2.new(1, 0.5)
				
				local InputCorner = Instance.new("UICorner", InputBox)
				InputCorner.CornerRadius = UDim.new(0, 6)
				
				local InputStroke = Instance.new("UIStroke", InputBox)
				InputStroke.Color = Color3.fromRGB(255, 0, 0)
				InputStroke.Thickness = 1
				
				local TextboxActual = Instance.new("TextBox", InputBox)
				TextboxActual.Size = UDim2.new(1, 0, 1, 0)
				TextboxActual.BackgroundTransparency = 1
				TextboxActual.TextColor3 = Color3.fromRGB(255, 255, 255)
				TextboxActual.PlaceholderColor3 = Color3.fromRGB(210,210,210)
				TextboxActual.PlaceholderText = "Input"
				TextboxActual.Font = Enum.Font.GothamSemibold
				TextboxActual.TextXAlignment = Enum.TextXAlignment.Center
				TextboxActual.TextSize = 14
				TextboxActual.ClearTextOnFocus = false
				TextboxActual.Text = TextboxConfig.Default
				
				local ClickButton = Instance.new("TextButton", TextboxFrame)
				ClickButton.Size = UDim2.new(1, 0, 1, 0)
				ClickButton.BackgroundTransparency = 1
				ClickButton.Text = ""

				AddConnection(TextboxActual:GetPropertyChangedSignal("Text"), function()
					InputBox.Size = UDim2.new(0, math.max(24, TextboxActual.TextBounds.X + 16), 0, 24)
				end)

				AddConnection(TextboxActual.FocusLost, function()
					TextboxConfig.Callback(TextboxActual.Text)
					if TextboxConfig.TextDisappear then
						TextboxActual.Text = ""
					end	
				end)

				AddConnection(ClickButton.MouseEnter, function()
					TweenService:Create(TextboxFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
				end)

				AddConnection(ClickButton.MouseLeave, function()
					TweenService:Create(TextboxFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.7}):Play()
				end)

				AddConnection(ClickButton.MouseButton1Up, function()
					TweenService:Create(TextboxFrame, TweenInfo.new(0.25, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0.5}):Play()
					TextboxActual:CaptureFocus()
				end)
			end
			
			function ElementFunction:AddColorpicker(ColorpickerConfig)
				ColorpickerConfig = ColorpickerConfig or {}
				ColorpickerConfig.Name = ColorpickerConfig.Name or "Colorpicker"
				ColorpickerConfig.Default = ColorpickerConfig.Default or Color3.fromRGB(255,255,255)
				ColorpickerConfig.Callback = ColorpickerConfig.Callback or function() end
				ColorpickerConfig.Flag = ColorpickerConfig.Flag or nil
				ColorpickerConfig.Save = ColorpickerConfig.Save or false

				local ColorH, ColorS, ColorV = 1, 1, 1
				local Colorpicker = {Value = ColorpickerConfig.Default, Toggled = false, Type = "Colorpicker", Save = ColorpickerConfig.Save}

				local ColorpickerFrame = Instance.new("Frame")
				ColorpickerFrame.Parent = ItemParent
				ColorpickerFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				ColorpickerFrame.BorderSizePixel = 0
				ColorpickerFrame.Size = UDim2.new(1, 0, 0, 38)
				ColorpickerFrame.BackgroundTransparency = 0.7
				ColorpickerFrame.ClipsDescendants = true
				
				local ColorpickerCorner = Instance.new("UICorner", ColorpickerFrame)
				ColorpickerCorner.CornerRadius = UDim.new(0, 6)
				
				local ColorpickerStroke = Instance.new("UIStroke", ColorpickerFrame)
				ColorpickerStroke.Color = Color3.fromRGB(255, 0, 0)
				ColorpickerStroke.Thickness = 1
				
				local ColorpickerText = Instance.new("TextLabel", ColorpickerFrame)
				ColorpickerText.Size = UDim2.new(1, -12, 1, 0)
				ColorpickerText.Position = UDim2.new(0, 12, 0, 0)
				ColorpickerText.BackgroundTransparency = 1
				ColorpickerText.TextColor3 = Color3.fromRGB(255, 255, 255)
				ColorpickerText.Font = Enum.Font.GothamBold
				ColorpickerText.TextSize = 15
				ColorpickerText.Text = ColorpickerConfig.Name
				ColorpickerText.TextXAlignment = Enum.TextXAlignment.Left
				
				local ColorBox = Instance.new("Frame", ColorpickerFrame)
				ColorBox.BackgroundColor3 = ColorpickerConfig.Default
				ColorBox.BorderSizePixel = 0
				ColorBox.Size = UDim2.new(0, 24, 0, 24)
				ColorBox.Position = UDim2.new(1, -12, 0.5, -12)
				ColorBox.AnchorPoint = Vector2.new(1, 0.5)
				
				local BoxCorner = Instance.new("UICorner", ColorBox)
				BoxCorner.CornerRadius = UDim.new(0, 6)
				
				local BoxStroke = Instance.new("UIStroke", ColorBox)
				BoxStroke.Color = Color3.fromRGB(255, 255, 255)
				BoxStroke.Thickness = 1
				
				local ClickButton = Instance.new("TextButton", ColorpickerFrame)
				ClickButton.Size = UDim2.new(1, 0, 0, 38)
				ClickButton.BackgroundTransparency = 1
				ClickButton.Text = ""
				
				-- Color picker container (akan muncul saat di-klik)
				local ColorPickerContainer = Instance.new("Frame", ColorpickerFrame)
				ColorPickerContainer.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				ColorPickerContainer.BorderSizePixel = 0
				ColorPickerContainer.Size = UDim2.new(1, 0, 0, 100)
				ColorPickerContainer.Position = UDim2.new(0, 0, 0, 38)
				ColorPickerContainer.BackgroundTransparency = 0.7
				ColorPickerContainer.Visible = false
				
				local ContainerCorner = Instance.new("UICorner", ColorPickerContainer)
				ContainerCorner.CornerRadius = UDim.new(0, 6)
				
				local ContainerStroke = Instance.new("UIStroke", ColorPickerContainer)
				ContainerStroke.Color = Color3.fromRGB(255, 0, 0)
				ContainerStroke.Thickness = 1
				
				-- Sederhana dulu, bisa dikembangkan nanti
				local InfoLabel = Instance.new("TextLabel", ColorPickerContainer)
				InfoLabel.Size = UDim2.new(1, -12, 1, 0)
				InfoLabel.Position = UDim2.new(0, 12, 0, 0)
				InfoLabel.BackgroundTransparency = 1
				InfoLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
				InfoLabel.Font = Enum.Font.Gotham
				InfoLabel.TextSize = 12
				InfoLabel.Text = "Click to open color picker\n(Coming soon)"
				InfoLabel.TextXAlignment = Enum.TextXAlignment.Left
				InfoLabel.TextWrapped = true

				AddConnection(ClickButton.MouseButton1Click, function()
					Colorpicker.Toggled = not Colorpicker.Toggled
					TweenService:Create(ColorpickerFrame,TweenInfo.new(.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{Size = Colorpicker.Toggled and UDim2.new(1, 0, 0, 138) or UDim2.new(1, 0, 0, 38)}):Play()
					ColorPickerContainer.Visible = Colorpicker.Toggled
				end)

				function Colorpicker:Set(Value)
					Colorpicker.Value = Value
					ColorBox.BackgroundColor3 = Colorpicker.Value
					ColorpickerConfig.Callback(Colorpicker.Value)
				end

				Colorpicker:Set(Colorpicker.Value)
				if ColorpickerConfig.Flag then				
					NexusUI.Flags[ColorpickerConfig.Flag] = Colorpicker
				end
				return Colorpicker
			end
			
			return ElementFunction   
		end	

		local ElementFunction = {}

		function ElementFunction:AddSection(SectionConfig)
			SectionConfig.Name = SectionConfig.Name or "Section"

			local SectionFrame = Instance.new("Frame", Container)
			SectionFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			SectionFrame.BorderSizePixel = 0
			SectionFrame.Size = UDim2.new(1, 0, 0, 28)
			SectionFrame.BackgroundTransparency = 1
			
			local SectionText = Instance.new("TextLabel", SectionFrame)
			SectionText.Size = UDim2.new(1, -12, 0, 16)
			SectionText.Position = UDim2.new(0, 0, 0, 4)
			SectionText.BackgroundTransparency = 1
			SectionText.TextColor3 = Color3.fromRGB(200, 200, 200)
			SectionText.Font = Enum.Font.GothamSemibold
			SectionText.TextSize = 14
			SectionText.Text = SectionConfig.Name
			SectionText.TextXAlignment = Enum.TextXAlignment.Left
			
			local Holder = Instance.new("Frame", SectionFrame)
			Holder.AnchorPoint = Vector2.new(0, 0)
			Holder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Holder.BorderSizePixel = 0
			Holder.Size = UDim2.new(1, 0, 1, -24)
			Holder.Position = UDim2.new(0, 0, 0, 23)
			Holder.BackgroundTransparency = 1
			Holder.Name = "Holder"
			
			local HolderList = Instance.new("UIListLayout", Holder)
			HolderList.SortOrder = Enum.SortOrder.LayoutOrder
			HolderList.Padding = UDim.new(0, 6)

			HolderList:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
				SectionFrame.Size = UDim2.new(1, 0, 0, HolderList.AbsoluteContentSize.Y + 31)
				Holder.Size = UDim2.new(1, 0, 0, HolderList.AbsoluteContentSize.Y)
			end)

			local SectionFunction = {}
			for i, v in next, GetElements(Holder) do
				SectionFunction[i] = v 
			end
			return SectionFunction
		end	

		for i, v in next, GetElements(Container) do
			ElementFunction[i] = v 
		end

		if TabConfig.PremiumOnly then
			-- Premium locked tab content
			for i, v in next, ElementFunction do
				ElementFunction[i] = function() end
			end    
			
			Container:ClearAllChildren()
			
			local PremiumFrame = Instance.new("Frame", Container)
			PremiumFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			PremiumFrame.BorderSizePixel = 0
			PremiumFrame.Size = UDim2.new(1, 0, 1, 0)
			PremiumFrame.BackgroundTransparency = 0.7
			
			local PremiumCorner = Instance.new("UICorner", PremiumFrame)
			PremiumCorner.CornerRadius = UDim.new(0, 6)
			
			local PremiumStroke = Instance.new("UIStroke", PremiumFrame)
			PremiumStroke.Color = Color3.fromRGB(255, 0, 0)
			PremiumStroke.Thickness = 1
			
			local LockIcon = Instance.new("ImageLabel", PremiumFrame)
			LockIcon.Size = UDim2.new(0, 18, 0, 18)
			LockIcon.Position = UDim2.new(0, 15, 0, 15)
			LockIcon.Image = "rbxassetid://3610239960"
			LockIcon.BackgroundTransparency = 1
			LockIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
			LockIcon.ImageTransparency = 0.4
			
			local LockText = Instance.new("TextLabel", PremiumFrame)
			LockText.Size = UDim2.new(1, -38, 0, 14)
			LockText.Position = UDim2.new(0, 38, 0, 18)
			LockText.BackgroundTransparency = 1
			LockText.TextColor3 = Color3.fromRGB(255, 255, 255)
			LockText.Font = Enum.Font.Gotham
			LockText.TextSize = 14
			LockText.Text = "Unauthorised Access"
			LockText.TextTransparency = 0.4
			LockText.TextXAlignment = Enum.TextXAlignment.Left
			
			local PremiumIcon = Instance.new("ImageLabel", PremiumFrame)
			PremiumIcon.Size = UDim2.new(0, 56, 0, 56)
			PremiumIcon.Position = UDim2.new(0.5, -28, 0.5, -40)
			PremiumIcon.Image = "rbxassetid://4483345875"
			PremiumIcon.BackgroundTransparency = 1
			PremiumIcon.ImageColor3 = Color3.fromRGB(255, 255, 255)
			
			local PremiumText = Instance.new("TextLabel", PremiumFrame)
			PremiumText.Size = UDim2.new(1, -20, 0, 14)
			PremiumText.Position = UDim2.new(0, 10, 0.5, -10)
			PremiumText.BackgroundTransparency = 1
			PremiumText.TextColor3 = Color3.fromRGB(255, 255, 255)
			PremiumText.Font = Enum.Font.GothamBold
			PremiumText.TextSize = 14
			PremiumText.Text = "Premium Features"
			PremiumText.TextXAlignment = Enum.TextXAlignment.Center
			
			local PremiumDesc = Instance.new("TextLabel", PremiumFrame)
			PremiumDesc.Size = UDim2.new(1, -40, 0, 40)
			PremiumDesc.Position = UDim2.new(0, 20, 0.5, 10)
			PremiumDesc.BackgroundTransparency = 1
			PremiumDesc.TextColor3 = Color3.fromRGB(200, 200, 200)
			PremiumDesc.Font = Enum.Font.Gotham
			PremiumDesc.TextSize = 12
			PremiumDesc.Text = "This part of the script is locked to Premium users."
			PremiumDesc.TextXAlignment = Enum.TextXAlignment.Center
			PremiumDesc.TextWrapped = true
			PremiumDesc.TextTransparency = 0.4
		end
		return ElementFunction   
	end  
	
	return TabFunction
end   

function NexusUI:Destroy()
	Nexus:Destroy()
end

function NexusUI:ToggleUi()
	Nexus.Enabled = not Nexus.Enabled
end

--==================================================
-- NEXUS UI TOGGLE BUTTON
-- ANTI DUPLICATE | TOP CENTER | DRAGGABLE
--==================================================

-- SERVICES
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

--==================================================
-- REMOVE OLD TOGGLE (ANTI DUPLICATE)
--==================================================
local oldToggle = playerGui:FindFirstChild("NexusUIToggle")
if oldToggle then
    oldToggle:Destroy()
end

--==================================================
-- SCREEN GUI
--==================================================
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "NexusUIToggle"
screenGui.Parent = playerGui
screenGui.ResetOnSpawn = false
screenGui.DisplayOrder = 9999

--==================================================
-- TOGGLE BUTTON
--==================================================
local toggleButton = Instance.new("ImageButton")
toggleButton.Parent = screenGui
toggleButton.Size = UDim2.new(0, 50, 0, 50)
toggleButton.BackgroundColor3 = Color3.fromRGB(25, 25, 30)
toggleButton.AutoButtonColor = false

-- POSISI ATAS TENGAH (LEBIH KE ATAS)
toggleButton.AnchorPoint = Vector2.new(0.5, 0)
toggleButton.Position = UDim2.new(0.5, 0, 0, 5)

-- ICON
toggleButton.Image = "http://www.roblox.com/asset/?id=82528573041751"

--==================================================
-- UI CORNER
--==================================================
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(0.25, 0)
uiCorner.Parent = toggleButton

--==================================================
-- UI STROKE
--==================================================
local uiStroke = Instance.new("UIStroke")
uiStroke.Parent = toggleButton
uiStroke.Color = Color3.fromRGB(100, 70, 255)
uiStroke.Thickness = 2
uiStroke.Transparency = 0.3

--==================================================
-- TOGGLE NEXUS UI
--==================================================
local isUIOpen = true

toggleButton.MouseButton1Click:Connect(function()
    if NexusUI and NexusUI.ToggleUi then
        NexusUI:ToggleUi()
        isUIOpen = not isUIOpen

        TweenService:Create(
            toggleButton,
            TweenInfo.new(0.25, Enum.EasingStyle.Quad),
            { Rotation = isUIOpen and 0 or 180 }
        ):Play()
    end
end)

--==================================================
-- HOVER EFFECT (PC SAFE)
--==================================================
toggleButton.MouseEnter:Connect(function()
    TweenService:Create(toggleButton, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(35, 35, 40),
        Size = UDim2.new(0, 52, 0, 52)
    }):Play()

    TweenService:Create(uiStroke, TweenInfo.new(0.2), {
        Transparency = 0
    }):Play()
end)

toggleButton.MouseLeave:Connect(function()
    TweenService:Create(toggleButton, TweenInfo.new(0.2), {
        BackgroundColor3 = Color3.fromRGB(25, 25, 30),
        Size = UDim2.new(0, 50, 0, 50)
    }):Play()

    TweenService:Create(uiStroke, TweenInfo.new(0.2), {
        Transparency = 0.3
    }):Play()
end)

--==================================================
-- DRAGGABLE (MOBILE + PC)
--==================================================
local dragging = false
local dragStart
local startPos

local function update(input)
    local delta = input.Position - dragStart
    toggleButton.Position = UDim2.new(
        startPos.X.Scale,
        startPos.X.Offset + delta.X,
        startPos.Y.Scale,
        startPos.Y.Offset + delta.Y
    )
end

toggleButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1
    or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = toggleButton.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

toggleButton.InputChanged:Connect(function(input)
    if dragging and (
        input.UserInputType == Enum.UserInputType.MouseMovement
        or input.UserInputType == Enum.UserInputType.Touch
    ) then
        update(input)
    end
end)

--==================================================
-- SAFETY
--==================================================
if not NexusUI then
    warn("NexusUI belum diload, toggle hanya visual.")
end

return NexusUI
